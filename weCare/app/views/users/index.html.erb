
<%= text_field_tag :search, params[:search] %>
<%= button_tag "Find robositters near me", id: "button" %>


<% if @babysitter_nearby %>
  <% @babysitter_nearby.each do |p| %>
    <li>
        <%= p.name %>
        <%= p.address %>
        <%= p.display_distance_to(@user, :km).round(2)%><p>kms away</p>
    </li>
  <%end%>
<%end%>
